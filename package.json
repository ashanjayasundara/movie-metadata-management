{
  "name": "axioms",
  "version": "1.0.0",
  "description": "Handle bulk metadata upload through Node.JS REST API",
  "main": "index.js",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "build": "tsc ",
    "dev": "ts-node-dev --respawn --pretty --transpile-only src/index.ts",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:create": "typeorm migration:create -n UserMigration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashanjayasundara/movie-metadata-management.git"
  },
  "keywords": [
    "Node.JS",
    "REST",
    "API",
    "OAUTH"
  ],
  "author": "Ashan Jayasundara",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ashanjayasundara/movie-metadata-management/issues"
  },
  "homepage": "https://github.com/ashanjayasundara/movie-metadata-management#readme",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.0",
    "@types/express-jwt": "^0.0.42",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mysql": "^2.15.19",
    "@types/node": "^16.11.8",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-jwt": "^6.1.0",
    "express-jwt-authz": "^2.4.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41"
  }
}
